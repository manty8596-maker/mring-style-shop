{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/submit-order.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/submit-order",
      "dest": "/api/submit-order"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}


